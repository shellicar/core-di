{
  "name": "@shellicar/core-di",
  "version": "2.0.0",
  "description": "A basic dependency injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellicar/core-di.git"
  },
  "private": false,
  "packageManager": "pnpm@9.15.2+sha512.93e57b0126f0df74ce6bff29680394c0ba54ec47246b9cf321f0121d8d9bb03f750a705f24edc3c1180853afd7c2c3b94196d0a3d53d3e069d9e2793ef11f321",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "test": "vitest run",
    "lint": "biome lint",
    "format": "biome format",
    "check": "biome check",
    "type-check": "tsc -p tsconfig.check.json",
    "ci": "biome ci",
    "ci:fix": "biome check --fix --diagnostic-level=error",
    "updates": "npm-check-updates",
    "postinstall": "lefthook install",
    "prepublishOnly": "run-p build ci",
    "postpublish": "pnpm version --no-git-tag-version patch"
  },
  "keywords": ["dependency injection", "di", "inversion of control container", "ioc"],
  "author": "Stephen Hellicar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shellicar/core-di/issues"
  },
  "homepage": "https://github.com/shellicar/core-di#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.5",
    "lefthook": "^1.10.1",
    "npm-check-updates": "^17.1.13",
    "npm-run-all2": "^7.0.2",
    "rimraf": "^6.0.1",
    "terser": "^5.37.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0"
  }
}
