{
  "name": "@shellicar/core-di",
  "version": "0.0.4",
  "description": "A basic dependency injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellicar/core-di.git"
  },
  "private": false,
  "packageManager": "pnpm@9.10.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "pkgroll --minify --sourcemap --clean-dist",
    "check": "tsc --noEmit --composite false --skipLibCheck",
    "test": "mocha",
    "lint": "eslint .",
    "prepublishOnly": "run-s clean build"
  },
  "keywords": [
    "dependency injection",
    "di",
    "inversion of control container",
    "ioc"
  ],
  "author": "Stephen Hellicar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shellicar/core-di/issues"
  },
  "homepage": "https://github.com/shellicar/core-di#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.8.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.5.5",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "eslint-plugin-turbo": "^2.1.2",
    "eslint-plugin-unicorn": "^55.0.0",
    "mocha": "^10.7.3",
    "npm-run-all2": "^6.2.3",
    "pkgroll": "^2.5.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0"
  }
}