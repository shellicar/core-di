{
  "name": "@shellicar/core-di",
  "version": "0.0.1",
  "description": "A basic dependency injection library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shellicar/core-di.git"
  },
  "private": false,
  "packageManager": "pnpm@9.9.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-p build:esm build:cjs build:types",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "check": "tsc --noEmit --composite false --skipLibCheck",
    "test": "mocha",
    "lint": "eslint .",
    "prepublishOnly": "run-s clean build"
  },
  "keywords": [
    "dependency injection",
    "di",
    "inversion of control container",
    "ioc"
  ],
  "author": "Stephen Hellicar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shellicar/core-di/issues"
  },
  "homepage": "https://github.com/shellicar/core-di#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.15",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.0",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "eslint-plugin-turbo": "^2.1.1",
    "eslint-plugin-unicorn": "^55.0.0",
    "mocha": "^10.7.3",
    "npm-run-all2": "^6.2.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.2"
  }
}